h === 0 && dispatch(moviesFailed(true))\r\n            } catch (e) {\r\n                console.log(e)\r\n                dispatch(getMoviesError(true))\r\n                dispatch(moviesLoading(false))\r\n            }\r\n        }\r\n        loadMorePages !== 1 && getMoviesData()\r\n    }, [loadMorePages])\r\n\r\n    const handleLoadMoreMovies = () => {\r\n        setLoadMorePages(prevState => prevState + 1)\r\n    }\r\n\r\n    return (\r\n        <Element className=\"categories\" name=\"categories\">\r\n            <div className=\"categories__buttons\">\r\n                {genres.length > 0 ? genres.map(genre => {\r\n                    return (\r\n                        <button\r\n                            className={pickedGenres.includes(genre.id) ? \"active\" : \"\"}\r\n                            onClick={() => handleGenres(genre.id)}\r\n                            key={genre.id}\r\n                            id={genre.id}\r\n                        >\r\n                            {genre.name}\r\n                        </button>\r\n                    )\r\n                }) : <Loading />}\r\n            </div>\r\n            <div className=\"categories__movies\">\r\n                {moviesByGenre.length > 0 &&\r\n                    moviesByGenre.map((movie, index) => {\r\n                        return (\r\n                            <MovieItem\r\n                                key={index}\r\n                                image={movie.poster_path && `${process.env.REACT_APP_IMAGE_URL}w500${movie.poster_path}`}\r\n                                title={movie.title}\r\n                                vote_average={movie.vote_average}\r\n                                id={movie.id}\r\n                            />\r\n                        )\r\n                    })\r\n                }\r\n            </div>\r\n            {movies_failed && <div className=\"categories__movies-failed\">\r\n                <h2>We couldnt't find movies with this specific genres, sorry!</h2>\r\n            </div>}\r\n            {moviesByGenre_error && <div className=\"categories__movies-error\">\r\n                <h2>Sorry, we are not able to display movies due to error, try refreshing the page!</h2>\r\n            </div>}\r\n            {!movies_failed && !moviesByGenre_error ? <div className=\"categories__loadmore-btn-wrapper\">\r\n                {hasNextPage && <button\r\n                    onClick={() => handleLoadMoreMovies()}\r\n                >\r\n                    Load more\r\n                </button>}\r\n            </div> : null}\r\n            <div className=\"categories__loading-wrapper\">\r\n                {movies_loading && <Loading size={90} />}\r\n            </div>\r\n            <GoUpButton\r\n                scrollToElementName={'categories'}\r\n            />\r\n        </Element>\r\n    )\r\n}\r\n\r\nexport default Genres\r\n","C:\\Users\\admin\\Desktop\\WORKSPACE ALL\\KK - MERN\\mern-movies-app\\client\\src\\redux\\actions\\genresActions.js",[],"C:\\Users\\admin\\Desktop\\WORKSPACE ALL\\KK - MERN\\mern-movies-app\\client\\src\\redux\\constants\\genresConstants.js",[],"C:\\Users\\admin\\Desktop\\WORKSPACE ALL\\KK - MERN\\mern-movies-app\\client\\src\\redux\\reducers\\genresReducer.js",[],"C:\\Users\\admin\\Desktop\\WORKSPACE ALL\\KK - MERN\\mern-movies-app\\client\\src\\components\\MoviesByCategories\\MoviesByCategories.js",["194","195","196"],"C:\\Users\\admin\\Desktop\\WORKSPACE ALL\\KK - MERN\\mern-movies-app\\client\\src\\components\\moviesByCategoriesBtns\\MoviesByCategoriesBtns.js",[],"C:\\Users\\admin\\Desktop\\WORKSPACE ALL\\KK - MERN\\mern-movies-app\\client\\src\\components\\moviesByCategories\\MoviesByCategories.js",["197","198"],"import React, { useEffect, useState } from 'react'\r\n\r\nimport axios from 'axios'\r\n\r\nimport MoviesByCategoriesBtns from '../moviesByCategoriesBtns/MoviesByCategoriesBtns'\r\nimport MovieItem from '../movieItem/MovieItem'\r\nimport Loading from '../loadin